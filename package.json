    "@react-native-community/eslint-config": "0.0.5",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/enzyme-to-json": "1.5.3",
    "@types/i18n-js": "3.0.1",
    "@types/jest": "24.0.17",
    "@types/node": "12.7.1",
