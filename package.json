{
  "name": "DemoApp",
  "version": "1.0.24",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start:ios": "react-native run-ios",
    "start:android": "react-native run-android",
    "test": "jest",
    "clear:all": "npm run clear:ios && npm run clear:android",
    "clear:ios": "cd ios && rm -rf Podfile.lock Pods/ && pod install",
    "clear:android": "cd android && ./gradlew clean",
    "release:android": "cd android && ./gradlew assembleRelease",
    "release:android:old": "react-native run-android --variant release",
    "eslint": "eslint .",
    "tslint": "tslint -t stylish -p tsconfig.json 'src/**/*.{ts,tsx}'",
    "lint": "npm run tslint",
    "lint:fix": "npm run tslint --fix"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.6.1",
