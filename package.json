{
  "name": "DemoApp",
  "version": "1.0.43",
  "private": true,
  "scripts": {
    "run:metro": "react-native start --reset-cache",
    "run:android": "concurrently --kill-others-on-fail -r 'yarn run:metro' \"react-native run-android --no-packager --appIdSuffix 'debug'\"",
    "start:ios2": "concurrently --kill-others-on-fail -r 'yarn run:metro' \"react-native run-ios --no-packager --simulator='iPhone 6'\"",
    "start:ios:iphonese": "concurrently --kill-others-on-fail -r 'yarn run:metro' \"react-native run-ios --no-packager --simulator='iPhone SE'\"",
    "start:ios:iphone8": "concurrently --kill-others-on-fail -r 'yarn run:metro' \"react-native run-ios --no-packager --simulator='iPhone 8'\"",
    "start:ios:iphone8plus": "concurrently --kill-others-on-fail -r 'yarn run:metro' \"react-native run-ios --no-packager --simulator='iPhone 8 Plus'\"",
    "start:ios:iphonex": "concurrently --kill-others-on-fail -r 'yarn run:metro' \"react-native run-ios --no-packager --simulator='iPhone X'\"",
    "start:ios:ipad": "concurrently --kill-others-on-fail -r 'yarn run:metro' \"react-native run-ios --no-packager --simulator='iPad Air 2'\"",
    "start:ios:ipad-pro": "concurrently --kill-others-on-fail -r 'yarn run:metro' \"react-native run-ios --no-packager --simulator='iPad Pro (12.9-inch) (2nd generation)'\"",
    "clear:yarn": "rm -rf node_modules yarn.lock && yarn cache clean --force && yarn",
    "clear:metro": "watchman watch-del-all && rm -rf $TMPDIR/react-* $TMPDIR/metro-bundler-cache-* && yarn run:metro --reset-cache",
    "start": "react-native start",
    "start:ios": "react-native run-ios --no-packager --simulator='iPhone 6'",
    "start:android": "react-native run-android",
    "clear:all": "yarn clear:yarn && yarn clear:android && yarn clear:ios && yarn clear:metro",
    "clear:ios": "(cd ios && rm -rf build Podfile.lock Pods/ && pod install)",
    "clear:android": "(cd android && ./gradlew clean)",
    "release:ios": "react-native run-ios --configuration Release",
    "release:android": "(cd android && ./gradlew assembleRelease && cd ..)",
    "release:android:old": "react-native run-android --variant release",
    "release:android:old2": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "eslint": "eslint .",
    "tslint": "tslint -t stylish -p tsconfig.json 'src/**/*.{ts,tsx}'",
    "lint": "yarn tslint",
    "lint:fix": "yarn tslint --fix",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.6.1",
    "@react-native-community/netinfo": "4.2.2",
    "fast-memoize": "2.5.1",
    "i18n-js": "3.3.0",
    "mobx": "5.13.0",
    "mobx-react": "6.1.3",
    "native-base": "2.13.8",
    "react": "16.9.0",
    "react-native": "0.60.5",
    "react-native-camera": "3.4.0",
    "react-native-classnames": "0.1.1",
    "react-native-gesture-handler": "1.4.1",
    "react-native-linear-gradient": "2.5.6",
    "react-native-localize": "1.2.1",
    "react-native-permissions": "1.2.1",
    "react-native-qrcode-scanner": "1.2.1",
    "react-native-reanimated": "1.2.0",
    "react-native-splash-screen": "3.2.0",
    "react-native-vector-icons": "6.6.0",
    "react-navigation": "4.0.6",
    "react-navigation-stack": "1.8.0",
    "reactotron-react-native": "4.0.0",
    "theme": "0.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-transform-flow-strip-types": "7.4.4",
    "@babel/plugin-transform-modules-commonjs": "7.6.0",
    "@babel/runtime": "7.6.0",
    "@gfx/zopfli": "1.0.14",
    "@react-native-community/eslint-config": "0.0.5",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/enzyme-to-json": "1.5.3",
    "@types/i18n-js": "3.0.1",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-native": "0.60.15",
    "@types/react-native-vector-icons": "6.4.2",
    "@types/react-navigation": "3.0.8",
    "@types/react-test-renderer": "16.9.0",
    "babel-jest": "24.9.0",
    "babel-plugin-ignite-ignore-reactotron": "0.3.0",
    "babel-plugin-module-resolver": "3.2.0",
    "concurrently": "4.1.2",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "lint-staged": "9.2.5",
    "marked": "0.7.0",
    "metro-react-native-babel-preset": "0.56.0",
    "prettier": "1.18.2",
    "react-native-dotenv": "0.2.0",
    "react-native-testing-library": "1.11.1",
    "react-test-renderer": "16.9.0",
    "ts-jest": "24.1.0",
    "tslint": "5.20.0",
    "tslint-config-airbnb": "5.11.2",
    "tslint-react": "4.1.0",
    "tslint-react-hooks": "2.2.1",
    "typescript": "3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm version patch",
      "post-commit": "git update-index --again"
    }
  }
}
