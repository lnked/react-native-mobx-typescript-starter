{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clear:cache": "rm -rf android/build/ ios/build/",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "run:ios": "react-native run-ios -- --reset-cache",
    "run:start": "react-native start --port=8088",
    "run:android": "react-native run-android",
    "run:ios:iphone5": "react-native run-ios --simulator 'iPhone 5'",
    "run:ios:app": "react-native run-ios --project-path './app/ios'",
    "run:ios:phone": "react-native run-ios --device 'Max iPhone'",
    "release:ios": "react-native run-ios --configuration Release",
    "build:ios": "react-native bundle --dev false --entry-file index.js --bundle-output ios/main.jsbundle --platform ios",
    "upgrade": "react-native-git-upgrade",
    "lint": "tslint -p . -t stylish",
    "test": "jest --coverage"
  },
  "dependencies": {
    "i18n-js": "3.2.1",
    "react": "16.8.3",
    "react-native": "0.59.1",
    "react-native-classnames": "0.1.1",
    "react-native-permissions": "2.0.0-alpha.1",
    "react-native-typescript-transformer": "1.2.12",
    "react-navigation": "3.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.4",
    "@babel/runtime": "7.3.4",
    "@types/jest": "24.0.11",
    "@types/react": "16.8.8",
    "@types/react-native": "0.57.40",
    "@types/react-test-renderer": "16.8.1",
    "babel-jest": "24.5.0",
    "babel-plugin-module-resolver": "3.2.0",
    "jest": "24.5.0",
    "metro-react-native-babel-preset": "0.53.1",
    "react-test-renderer": "16.8.3",
    "tslint": "5.14.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "3.6.0",
    "tslint-react-recommended": "1.0.15",
    "typescript": "3.3.3333"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./src/resources/fonts/"
    ]
  }
}
