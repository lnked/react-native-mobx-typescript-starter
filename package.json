{
  "name": "app",
  "version": "0.1.28",
  "private": true,
  "scripts": {
    "clear:watch": "watchman watch-del-all",
    "clear:cache": "rm -rf android/build/ ios/build/",
    "clear:react-native": "rm -rf $TMPDIR/react-native-packager-cache-*",
    "clear:metro": "rm -rf $TMPDIR/metro-bundler-cache-*",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "poststart": "yarn react-devtools",
    "debugger": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "run:ios": "react-native run-ios -- --reset-cache",
    "run:start": "react-native start --port=8088",
    "run:android": "react-native run-android -- --reset-cache",
    "run:ios:iphone5": "react-native run-ios --simulator 'iPhone 5'",
    "run:ios:app": "react-native run-ios --project-path './app/ios'",
    "run:ios:phone": "react-native run-ios --device 'Max iPhone'",
    "release:ios": "react-native run-ios --configuration Release",
    "bundle:ios": "react-native bundle --dev false --entry-file index.js --bundle-output ios/main.jsbundle --platform ios",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output index.bundle",
    "bundle:android2": "cd android && ./gradlew assembleRelease",
    "clear:yarn": "rm -rf node_modules yarn.lock && yarn cache clean --force && yarn",
    "clear:ios": "(cd ios && rm -rf build Podfile.lock Pods/* && pod install)",
    "clear:ios2": "rm -rf ios/build/ && kill (lsof -t -i:8081) && react-native run-ios",
    "clear:android": "(cd android && ./gradlew clean)",
    "clear:all": "yarn clear:yarn && yarn clear:android && yarn clear:ios && yarn clear:cache && yarn clear:watch && yarn clear:metro && yarn clear:react-native",
    "tsc": "tsc -b",
    "lint": "tslint -t stylish -p tsconfig.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "react-devtools": "react-devtools",
    "coverage": "rimraf coverage && jest --coverage",
    "xcode": "open ios/app.xcodeproj",
    "rninit": "react-native init RN059 --version react-native@next",
    "configure": "./node_modules/react-native/third-party/glog-0.3.5/configure",
    "studio": "open -a /Applications/Android\\ Studio.app ./android/"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.4.2",
    "@react-native-community/netinfo": "3.2.1",
    "apisauce": "1.0.2",
    "i18n-js": "3.2.2",
    "mobx": "5.10.0",
    "mobx-react": "6.0.3",
    "react": "16.8.6",
    "react-native": "0.59.9",
    "react-native-animatable": "1.3.2",
    "react-native-classnames": "0.1.1",
    "react-native-device-info": "2.1.0",
    "react-native-fs": "2.13.3",
    "react-native-gesture-handler": "1.3.0",
    "react-native-languages": "3.0.2",
    "react-native-permissions": "2.0.0-alpha.1",
    "react-native-splash-screen": "3.2.0",
    "react-native-svg": "9.4.0",
    "react-native-typescript-transformer": "1.2.12",
    "react-native-vector-icons": "6.4.2",
    "react-navigation": "3.11.0",
    "react-navigation-drawer": "1.3.0",
    "react-navigation-stack": "1.4.0",
    "react-navigation-tabs": "2.1.3",
    "reactotron-react-native": "3.6.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-transform-flow-strip-types": "7.4.4",
    "@babel/plugin-transform-modules-commonjs": "7.4.4",
    "@babel/runtime": "7.4.5",
    "@types/enzyme": "3.9.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/enzyme-to-json": "1.5.3",
    "@types/i18n-js": "3.0.1",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.5",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/react-native": "0.57.60",
    "@types/react-native-vector-icons": "6.4.1",
    "@types/react-navigation": "3.0.7",
    "@types/react-test-renderer": "16.8.1",
    "babel-plugin-ignite-ignore-reactotron": "0.3.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-preset-mobx": "2.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.3.5",
    "husky": "2.4.0",
    "jest": "24.8.0",
    "lint-staged": "8.1.7",
    "metro-react-native-babel-preset": "0.54.1",
    "react-devtools": "3.6.1",
    "react-native-dotenv": "0.2.0",
    "react-native-jest-mocks": "1.4.0",
    "react-native-testing-library": "1.8.0",
    "react-test-renderer": "16.8.6",
    "ts-jest": "24.0.2",
    "tslint": "5.17.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-react": "4.0.0",
    "tslint-react-hooks": "2.1.0",
    "typescript": "3.5.1",
    "webpack": "4.33.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm version patch"
    }
  },
  "rnpm": {
    "assets": [
      "./src/resources/fonts/"
    ]
  }
}
